{
  "name": "silverware/validator",
  "type": "silverstripe-vendormodule",
  "description": "SilverWare Validator Module.",
  "homepage": "https://github.com/praxisnetau/silverware-validator",
  "keywords": [
    "silverware",
    "validator",
    "validation",
    "parsley",
    "forms",
    "silverstripe"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Colin Tucker",
      "role": "Developer",
      "email": "colin@praxis.net.au",
      "homepage": "https://www.praxis.net.au"
    }
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7",
    "silverstripe/framework": "^5"
  },
  "autoload": {
    "psr-4": {
      "SilverWare\\Validator\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
